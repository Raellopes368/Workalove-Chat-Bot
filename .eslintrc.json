{
    "env": {
        "browser": true,
        "es2021": true
    },

    "extends": [
        "airbnb",
        "prettier"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "prettier"
    ],
    "rules": {
      "prettier/prettier": "error",
      "react/jsx-filename-extension": [
        "warn",
        { "extensions": [".jsx", ".js"]}
      ],
      "import/prefer-default-export": "off",
      "no-console": "off"
    }
}
